@import "./config.rasi"

@theme "/dev/null"
@import "../themes/colors.rasi"
@import "../menu-conf.rasi"

window {
  location: south east;
  width: 30%;
  x-offset: 0%;
  y-offset: 0;
  height: 40%;
  background-color: transparent;
  border-radius: 8px;
}

listview, element, element selected, element-text, element-icon {
  padding: 4px;
  background-color: transparent;
}
